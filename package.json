{
  "name": "fostr",
  "version": "0.0.1",
  "description": "Free and open source tournament recorder",
  "main": "index.js",
  "scripts": {
    "dev": "rm -rf build/ && webpack -c ./webpack.electron.config.js && concurrently \"webpack s -c ./webpack.web.config.js\" \"wait-on http://localhost:9000/ && cross-env NODE_ENV=development ELECTRON_DISABLE_SECURITY_WARNINGS=true electron ./build/electron.js\"",
    "build": "rm -rf build/ && webpack -c ./webpack.electron.config.js --mode=production && webpack -c ./webpack.web.config.js --mode=production",
    "build-linux": "npm run build && electron-builder -c ./electron-builder.config.json -l",
    "build-mac": "npm run build && electron-builder -c ./electron-builder.config.json -m",
    "build-windows": "npm run build && electron-builder -c ./electron-builder.config.json -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slashinfty/FOSTR.git"
  },
  "keywords": [
    "tournament"
  ],
  "author": "Matt Braddock",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/slashinfty/FOSTR/issues"
  },
  "homepage": "https://github.com/slashinfty/FOSTR#readme",
  "devDependencies": {
    "@types/pdfmake": "^0.1.21",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^18.0.2",
    "electron-builder": "^22.14.13",
    "electron-context-menu": "^3.1.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-store": "^8.0.1",
    "html-webpack-plugin": "^5.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.0"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.5",
    "@mui/icons-material": "^5.6.0",
    "@mui/material": "^5.6.0",
    "@vighnesh153/use-global-state": "^1.3.89",
    "notistack": "^2.0.4",
    "pdfmake": "^0.2.5",
    "react-hotkeys-hook": "^3.4.4",
    "tournament-organizer": "^2.1.7"
  }
}
